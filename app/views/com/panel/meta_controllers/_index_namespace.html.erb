<% @name_spaces.each do |name_space| %>
<tr id="tr_<%= params[:business_identifier] %>_<%= name_space.identifier.blank? ? '_' : name_space.identifier %>" data-controller="tree-remote" data-tree-remote-target="item">
  <td>
    <span style="width: 1.43em; display: inline-block"></span>
    <%= button_to({ action: 'governs', business_identifier: @busyness.identifier, namespace_identifier: name_space.identifier }, data: { turbo_frame: 'visit' }) do %>
      <i class="fas fa-fw fa-caret-right" style="cursor: pointer" data-action="click->tree-remote#expand"></i>
    <% end %>
    <span><%= name_space.name %></span>
  </td>
  <td></td>
</tr>
<% end %>
