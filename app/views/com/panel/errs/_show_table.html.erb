<div>
  <dt><%= Com::Err.human_attribute_name(:created_at) %></dt>
  <dd><time data-controller="time"><%= @err.created_at.to_fs(:iso8601) %></time></dd>
</div>
<div>
  <dt><%= Com::Err.human_attribute_name(:exception_object) %></dt>
  <dd><%= @err.exception_object %></dd>
</div>
<div>
  <dt><%= Com::Err.human_attribute_name(:path) %></dt>
  <dd><%= @err.path %></dd>
</div>
<div>
  <dt><%= Com::Err.human_attribute_name(:controller_name) %></dt>
  <dd><%= @err.controller_name %></dd>
</div>
<div>
  <dt><%= Com::Err.human_attribute_name(:action_name) %></dt>
  <dd><%= @err.action_name %></dd>
</div>
<div>
  <dt><%= Com::Err.human_attribute_name(:params) %></dt>
  <dd><%= simple_format @err.params %></dd>
</div>
<div>
  <dt><%= Com::Err.human_attribute_name(:cookie) %></dt>
  <dd><%= simple_format @err.cookie %></dd>
</div>
<div>
  <dt><%= Com::Err.human_attribute_name(:session) %></dt>
  <dd><%= simple_format @err.session %></dd>
</div>
<div>
  <dt><%= Com::Err.human_attribute_name(:headers) %></dt>
  <dd><%= simple_format @err.headers %></dd>
</div>
<div>
  <dt><%= Com::Err.human_attribute_name(:exception) %></dt>
  <dd><pre><%= @err.exception %></pre></dd>
</div>
<div>
  <dt><%= Com::Err.human_attribute_name(:exception_backtrace) %></dt>
  <dd><pre style="overflow-x: auto"><%= @err.exception_backtrace.join("\n") %></pre></dd>
</div>
