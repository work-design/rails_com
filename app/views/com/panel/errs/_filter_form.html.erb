<%= form_with theme: 'search', model: Com::Err.new, url: { action: params[:action] } do |f| %>
  <div class="field-body">
    <%= f.text_field :controller_name %>
    <%= f.text_field :action_name %>
    <%= f.text_field 'path-like' %>
    <%= f.text_field :exception_object %>
    <div class="field is-narrow">
      <%= f.submit %>
    </div>
  </div>
<% end %>
