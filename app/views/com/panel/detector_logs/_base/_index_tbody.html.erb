<td><%= model.status %></td>
<% if model.error.present? %>
  <td><%= model.error %></td>
<% else %>
  <td><%= model.body %></td>
<% end %>
<td>
  <p><time data-controller="time" data-format="human"><%= model.started_at.to_fs(:iso8601) %></time></p>
  <p><time data-controller="time" data-format="human"><%= model.created_at.to_fs(:iso8601) %></time></p>
</td>
