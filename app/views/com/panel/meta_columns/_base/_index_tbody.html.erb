<td><%= model.column_name %></td>
<td>
  <p><%= model.column_type %></p>
  <p class="text-grey"><%= model.sql_type %></p>
</td>
<td><%= model.column_limit %></td>
<td>
<% if model.defined_db %>
  <i class="fa-solid fa-check text-success"></i>
<% else %>
  <i class="fa-solid fa-times text-danger"></i>
<% end %>
</td>
<td>
<% if model.defined_model %>
  <i class="fa-solid fa-check text-success"></i>
<% else %>
  <i class="fa-solid fa-times text-danger"></i>
<% end %>
</td>
