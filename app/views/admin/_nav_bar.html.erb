<div class="navbar-breadcrumb" style="margin-bottom: 0.75rem">
  <div class="breadcrumb has-succeeds-separator has-background-light">
    <ul>
      <%= render 'breadcrumb' %>
    </ul>
  </div>
  <nav data-controller="navbar" class="navbar is-light">
    <div data-navbar-target="menu" class="navbar-menu">
      <div class="navbar-end">
        <%= render partial: 'locales' %>
        <%= render partial: 'notice_navbar' if defined? RailsNotice %>
        <%= render partial: 'admin/auth_navbar' if current_member %>
      </div>
    </div>
  </nav>
</div>
