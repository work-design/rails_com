<td>
  <p>
    <%= link_to({ controller: 'jobs', action: 'show', id: model.id }) do %>
      <%= model.id %>
    <% end %>
  </p>
  <p><%= model.class_name %></p>
  <p><time data-controller="time" data-format="human"><%= model.created_at.to_fs(:iso8601) %></time></p>
  <p class="text-info">
    <time data-controller="time" data-format="human"><%= model.scheduled_at.to_fs(:iso8601) %></time>
    <span data-controller="time-count" data-time-count-time-value="<%= model.scheduled_at.to_fs(:iso8601) %>" class="tag"><%= exact_distance_time(model.scheduled_at, Time.current) %></span>
  </p>
  <pre><%= model.arguments['arguments'].to_format_yaml %></pre>
</td>
<td>
  <p class="text-danger"><%= model.failed_execution.error['message'] %></p>
</td>
