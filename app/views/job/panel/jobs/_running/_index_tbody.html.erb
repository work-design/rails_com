<td>
  <p>
    <%= link_to({ action: 'show', id: model.id }) do %>
      <%= model.id %>
    <% end %>
  </p>
  <p><span class="tag"><%= model.status %></span></p>
</td>
<td>
  <p><%= model.class_name %></p>
  <p
    data-controller="time-count"
    data-time-count-time-value="<%= model.scheduled_at.to_fs(:iso8601) %>"
  ><%= exact_distance_time(model.scheduled_at, Time.current) %></p>
</td>
<td>
  <pre><%= model.arguments['arguments'][0].to_format_yaml %></pre>
</td>
